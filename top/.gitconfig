[user]
	name = Tom Zakrajsek
	email = tzakrajsek@gmail.com
	useConfigOnly = true
[color]
	ui = auto
[alias]
	unstage = reset HEAD --
	last = log -1 HEAD
	st = status
	co = checkout
	lg = log --oneline --graph -10
	dc = diff --cached
        ds = diff --stat
        dsc = diff --stat --cached
[push]
	default = simple

[branch]
	autosetuprebase = always
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
